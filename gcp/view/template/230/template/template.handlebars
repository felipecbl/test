								<li data-delivery="email" data-dnature='electronic'><span> {{{customize.email.title}}} </span></li>
								<li data-delivery="print" data-dnature='electronic'><span> {{{customize.print.title}}} </span></li>
								<li data-delivery="facebook" data-dnature='electronic'><span> {{{customize.facebook.title}}} </span></li>
								<li data-delivery="sms" data-dnature='electronic'><span> {{{customize.sms.title}}} </span></li>
							</ul>
							{{! <!-- Select the delivery type end --> }}
							
							{{! <!-- Delivery type content. It changes depending on the selection --> }}
