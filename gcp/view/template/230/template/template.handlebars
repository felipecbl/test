
{{! <!-- the 'wrapper' holds everything together --> }}
<div id='wrapper'>
	
	{{! <!-- Preview Window --> }}
	<div id='preview-window-bg'>
		<div id='preview-window'>
			<img id='preview-image' src=''>
			<span id="pw-close">x</span>
			<span id="pw-value"></span>
			<span id="pw-message"></span>
		</div>
	</div>
	{{! <!-- Preview Window end --> }}

	{{! Confirmation page }}
	<div id="confirmation-window-bg">
		<div id="confirmation-window">

			<div id="cw-content">
				<div class="summary">
					<h2>{{{confirmation.order_summary}}}</h2>
					<div class='cw-order'><div>{{{confirmation.order_total}}}</div><span></span></div>
					<div class='cw-shipping'><div>{{{confirmation.shipping_total}}}</div><span></span></div>
					<div class='cw-discount'><div>{{{confirmation.discount_total}}}</div><span></span></div>
					<div class='cw-total'><div>{{{confirmation.total}}}</div><span></span></div>
				</div>
				<div class="summary">
					<h2>{{{confirmation.card_info}}}</h2>
					<div class="cw-type"><div>{{{confirmation.card_type}}}</div><span></span></div>
					<div class="cw-number"><div>{{{confirmation.card_number}}}</div><span></span></div>
					<div class="cw-date"><div>{{{confirmation.card_expiration}}}</div><span></span></div>
				</div>
				<div class="summary hundred-percent">
					<h2>{{{confirmation.billing_address}}}</h2>
					<div class="cw-email"><div>{{{confirmation.email_address}}}</div><span></span></div>
					<div class="cw-phone"><div>{{{confirmation.phone}}}</div><span></span></div>
					<div class="cw-address"><div>{{{confirmation.address}}}</div><span></span></div>
					<div class="cw-city"><div>{{{confirmation.city}}}</div><span></span></div>
					<div class="cw-state"><div>{{{confirmation.state}}}</div><span></span></div>
					<div class="cw-country"><div>{{{confirmation.coutry}}}</div><span></span></div>
					<div class="cw-zip"><div>{{{confirmation.zip}}}</div><span></span></div>
				</div>
			</div>

			{{! <!-- Pages Navigation --> }}
			<div class="pages-nav">
				<div class='group-error'></div>
				<a href="javascript:void(0)" class='button back-button back-to-purchaser-info'>
					<span>{{{checkout.text.back_to_purchaser_info}}}</span>
				</a>

				<a href="javascript:void(0)" class='button continue-button go-to-summary'>
					<span>{{{checkout.text.go_to_purchaser_info}}}</span>
				</a>
			</div>
			{{! <!-- Pages Navigation end --> }}
		</div>
	</div>	
	{{! Confirmation Page end }}



	{{! <!-- Status bar --> }}
	<header>
		<ul id='status-bar'>
			<li id='header-customize' class='status-page'>
				<div class='theme-color-bg'><div>1</div></div>
				<div class='page-title'> {{{customize.text.title}}} </div>
			</li> 
			<li class='separator'></li>
			<li id='header-checkout'  class='status-page'>
				<div class='theme-color-bg'><div>2</div></div>
				<div class='page-title'> {{{checkout.text.title}}} </div>
			</li>
		</ul>
	</header>
	{{! <!-- Status bar end --> }}

	{{! <!-- Content --> }}
	<div id='content'>	


		{{! <!-- Pages Wrapper --> }}
		<div id='pages-wrapper'>
		{{!-- <h1>Swiss√¥tel Gift Cards</h1>--}}
			<ul id="pages">
				{{! <!-- Page 1 (customize card(s) ) --> }}
				<li id='customize' class='validate' data-step='step1'>
					
					{{! <!-- Fields --> }}
					<ul class='fields'>
						{{! <!-- Card info --> }}
						<li id='customize_card' class='tab-group'>
							<h2> {{{customize.text.customize_card}}} </h2>
							{{! <!-- Card selector --> }}

							{{! Design selector }}
							{{! tab }}
							<ul id='custom-type'  class='dnumber-2'>
								<input type="hidden" name='customize-type' id='customize-type' value='default'>
								<li data-ctype='default' class='dt-active' data-id='0'>Choose a design</li>
								<li data-ctype='photo' data-id='1'>Upload a photo</li>
							</ul>
							{{! tab end }}

							{{! Design panels }}
							<ul id="design-selector">
								
								{{! Choose Design }}
								<li id='choose-design' class='validate'>									
									{{! Top Image Wrap }}
									<figure id='image-wrap'></figure>
									{{! Top Image Wrap end }}				

									<div id='card-selector'>
										{{! Top Image Wrap }}
										<div id='image-wrap'></div>
										{{! Top Image Wrap end }}

										<div class='field-left'>
											{{! <!-- Card image. on click shows the carousel --> }}
											<div class='choose-card'><span>{{{customize.text.change_card}}}</span></div>
											{{! <!-- Default value = 1 > chainID/images/card/1.png --> }}
											<input type="hidden" name='card_design' id='card_design'>
											<input type="hidden" name='card_src' id='card_src'>
										</div>
										<div class='field-right'>
											<a href='javascript:void(0)' class='button change_design_bt'>
												<span>{{{customize.text.change_design_bt}}}</span>
											</a>
											<a href='javascript:void(0)' class='button preview_bt'>
												<span>{{{customize.text.preview_bt}}}</span>
											</a>
											<div class='quantity half-field'> {{! validation-error }}
												<label for='card_quantity'> {{{customize.label.quantity}}} </label>
												<div>
													<input type='text' name='card_quantity' id='card_quantity' class='quantity up-down-number number' value='1'> 
												</div>
												<div class='field-error'></div>
											</div>
										</div>
									</div>
									{{! <!-- Card selector end --> }}
								</li>
								{{! Choose Design end}}

								{{! Upload photo }}
								<li id='upload-photo'>
									{{! <h2>Instructions</h2> }}
									<p>Add a photo clicking on the blank card and then crop on the side.</p>

									<div id="photo-selector">
										
										<div class='field-left'>
											{{! Photo wrap }}
											<div id='photo-wrap'> <div id="photo-in"></div> <span>Click to preview</span></div>
											{{! Photo wrap end}}
											<div class='half-field'>
												<div class='field-error'></div>
												<input id='saved-photo' type="hidden" class='required photo' />
											</div>
										</div>

										<div class='field-right'>
											<div id="up-panel">												
												<a class="button add_photo" href="javascript:void(0)">
													<span>{{{customize.label.browse_photo}}}</span>
													<div id="p-bar"></div>
													<input id='photo-file' type="file" class='hide-it' name="files[]" accept="image/jpg" />
												</a>
											</div>
										</div>
									</div>
								</li>
								{{! Upload photo end}}

							</ul>
							{{! Design panels end }}

							{{! Design selector end }}

							<div id="customize-data">
								{{! <!-- Message field --> }}
								<div class='field'>
									<div class='field-left'>
										<div>
											<label for='card_message'> {{{customize.label.message}}} </label>
										</div>
										<div class='field-error'></div>
									</div>
									<div class='field-right'>
										<div class='input-holder'>
											<textarea name='card_message' id='card_message' cols='30' rows='10' class='required message' placeholder='{{{customize.placeholder.message}}}'></textarea>
										</div>									
										<div class='field-detail'><div id='chars'></div><div id='char-line'> {{{customize.detail.message}}} (</div><div id='chars-lines'> </div> {{{customize.detail.message_lines}}} )</div>
									</div>
								</div>
								{{! <!-- Message field end --> }}

								{{! <!-- Ammount field --> }}
								<div class='field'>
									<div class='field-left'>
										<div>
											<label for='card_value'> {{{customize.label.value}}} </label>
										</div>
										<div class='field-error'></div>
									</div>
									<div class='field-right'>
										<div class='input-holder'>
											<input type='text' name='card_value' id='card_value' class='required ammount number no-tab' placeholder='{{{customize.placeholder.value}}}'><span class='dollar-sign'>$</span>
											<div class='field-detail'> {{{customize.detail.value}}} </div>
											<div class='field-detail-value'>{{{customize.detail.value3}}}</div>
										</div>
										<div class='values-wrap'>
											{{! <!-- Buttons with values here --> }}
										</div>
									</div>
								</div>
								{{! <!-- Ammount field end --> }}
							</div>

							{{! <!-- Pages Navigation --> }}
							<div class="pages-nav customize-nav">
								<div class='group-error'></div>
								<a href="javascript:void(0)" class='button continue-button go-to-delivery no-tab'>
									<span>{{{customize.text.go_to_delivery}}}</span>
								</a>
							</div>
							{{! <!-- Pages Navigation end --> }}

						</li>
						{{! <!-- Card info end --> }}

						{{! <!-- Delivery info --> }}
						<li id='delivery-info' class='tab-group'>
							<h2> {{{customize.text.delivery_info}}} </h2>
							{{! <!-- Select the delivery type  --> }}
							<ul id='delivery-type'>
								<input type="hidden" name='delivery_type' id='delivery_type' value='email'>
